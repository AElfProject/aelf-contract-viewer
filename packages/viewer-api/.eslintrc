{
  "root": false,
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": false,
    "jest": true
  },
  "plugins": [
    "babel",
    "import"
  ],
  "extends": [
    "eslint-config-airbnb-base",
    "eslint-config-egg"
  ],
  "globals": {},
  "rules": {
    "comma-dangle": "off",
    "import/no-extraneous-dependencies": "off"
  }
}
